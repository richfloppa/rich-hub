InstantKill.MouseButton1Down:Connect(function()
	for i, v in pairs(workspace:GetChildren()) do
		if v.Name == "GuideNPC" then
			local GuideNPC = v
			local hum = GuideNPC:FindFirstChild("Humanoid")
			if hum then hum.Health = 0 end
		end
	end
end)
