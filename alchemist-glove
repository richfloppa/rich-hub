game:GetService("ReplicatedStorage").AlchemistEvent:FireServer("BrewPotion")
end

local function LethalPotion()
for i = 1, 10 do
    LethalAnd()
end
game:GetService("ReplicatedStorage").AlchemistEvent:FireServer("BrewPotion")
end

local function IdiotPotion()
mixItem("Cake Mix")
game:GetService("ReplicatedStorage").AlchemistEvent:FireServer("BrewPotion")
end

