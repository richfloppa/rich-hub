local OrionLib = loadstring(game:HttpGet(("https://raw.githubusercontent.com/Giangplay/Script/main/Orion_Library_PE_V2.lua")))()

-- get a reference to the replicated storage service
local replicatedStorage = game:GetService("ReplicatedStorage")

-- find the "BrazilHit" script
local brazilHitScript = replicatedStorage:FindFirstChild("BrazilHit")

if brazilHitScript then
    OrionLib:MakeNotification({
        Name = "Rich Hub",
        Content = "Script found.",
        Image = "rbxassetid://7733675275",
        Time = 5
    })
end