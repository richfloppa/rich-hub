  Name = "Slap Battles Hub That Exist",
  Callback = function()   --[[
	WARNING: Heads up! This script has not been verified by ScriptBlox. Use at your own risk!
]]
loadstring(game:HttpGet(("https://raw.githubusercontent.com/ionlyusegithubformcmods/1-Line-Scripts/main/Slap%20Battles")))() 
    end    
})

Tab:AddButton({
  Name = "Fly GUI V3",
  Callback = function()  loadstring(game:HttpGet("https://raw.githubusercontent.com/XNEOFF/FlyGuiV3/main/FlyGuiV3.txt"))()
    end    
})

Tab:AddButton({
  Name = "Slap Battles OP Script",
  Callback = function()  loadstring(game:HttpGet("https://raw.githubusercontent.com/Giangplay/Slap_Battles/main/Slap_Battles.lua"))()
    end    
})

Tab:AddButton({
