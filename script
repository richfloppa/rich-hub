local Targets = {} -- Stores usernames to fling
local AutoFling = false -- Toggle for auto flinging

local Targets = {} -- Stores usernames to fling
local AutoFling = false -- Toggle for auto flinging

Tab:AddTextbox({
    Name = "Target Username",
    Default = "Enter username here",
    TextDisappear = true,
    Callback = function(Value)
        Targets[1] = Value
    end	  
})

Tab:AddToggle({
    Name = "Auto Fling",
    Default = false,
    Callback = function(Value)
        AutoFling = Value
    end    
})

local Players = game:GetService("Players")
local Player = Players.LocalPlayer

local function GetPlayer(Name)
    Name = Name:lower()
    if Name == "all" or Name == "others" then
    AllBool = true
    return nil
elseif Name == "random" then
        local GetPlayers = Players:GetPlayers()
        if table.find(GetPlayers, Player) then table.remove(GetPlayers, table.find(GetPlayers, Player)) end
        return GetPlayers[math.random(#GetPlayers)]
    elseif Name ~= "random" and Name ~= "all" and Name ~= "others" then
        for _,x in next, Players:GetPlayers() do
            if x ~= Player then
                if x.Name:lower():match("^"..Name) then
                    return x;
                elseif x.DisplayName:lower():match("^"..Name) then
                    return x;
                end
            end
        end
    else
        return
    end
end

local function Message(_Title, _Text, Time)
    game:GetService("StarterGui"):SetCore("SendNotification", {Title = _Title, Text = _Text, Duration = Time})
end

local function SkidFling(TargetPlayer)
    local Character = Player.Character
    local Humanoid = Character and Character:FindFirstChildOfClass("Humanoid")
    local RootPart = Humanoid and Humanoid.RootPart

    local TCharacter = TargetPlayer.Character
    local THumanoid
    local TRootPart
    local THead
    local Accessory
    local Handle

    if TCharacter:FindFirstChildOfClass("Humanoid") then
        THumanoid = TCharacter:FindFirstChildOfClass("Humanoid")
    end
    if THumanoid and THumanoid.RootPart then
        TRootPart = THumanoid.RootPart
    end
    if TCharacter:FindFirstChild("Head") then
        THead = TCharacter.Head
    end
    if TCharacter:FindFirstChildOfClass("Accessory") then
        Accessory = TCharacter:FindFirstChildOfClass("Accessory")
    end
    if Accessory and Accessory:FindFirstChild("Handle") then
        Handle = Accessory.Handle
    end

    if Character and Humanoid and RootPart then
        -- Insert flinging logic here
        -- This part is integrated from the second script
        -- You can replace this with your actual flinging logic
        print("Flinging", TargetPlayer.Name)
    else
        Message("Error Occurred", "Random error", 5)
    end
end

local function IsPlayerWhitelisted(player)
    local userId = player.UserId
    local WhitelistedPlayers = {
        [3891242429] = true, -- Replace with the actual UserId of whitelisted players
        [4755369321] = true,
    }
    return WhitelistedPlayers[userId] or false
end

while true do
    if AutoFling then
        for _, target in ipairs(Targets) do
            local TargetPlayer = GetPlayer(target)
            if TargetPlayer then
                Message("User Selected", TargetPlayer.Name .. " is selected as target", 3)
                if not IsPlayerWhitelisted(TargetPlayer) then
                    SkidFling(TargetPlayer)
                else
                    Message("Info", "This user is whitelisted. Skipping fling.", 5)
                end
            else
                Message("Error Occurred", target .. " not found in the server. Script will not work.", 5)
            end
        end
        wait(10)  -- Fling every 10 seconds
    else
        wait(1) -- Check every second if auto flinging is enabled
    end
end

OrionLib:Init() 